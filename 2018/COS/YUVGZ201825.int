Part1(FileName="",RecordsArr) PUBLIC	;
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line),w=+$piece(Record,",",1),x=+$piece(Record,",",2),y=+$piece(Record,",",3),z=+$piece(Record,",",4)
		set DotsArr(Line)=$listbuild(w,x,y,z)
		set DistArr($zabs(w)+$zabs(x)+$zabs(y)+$zabs(z),Line)=""
		//set DistArr(w+x+y+z,Line)=""
		//set DistArr($zabs(w+x+y+z),Line)=""
	}
	zwrite DotsArr,DistArr
	do MakeConstellation(.DotsArr,.DistArr,.constellationArr)
	for  quit:'$$CompressConstellation(.DotsArr,.constellationArr)
	zwrite constellationArr
	K ^DEBUG m ^DEBUG("constellationArr")=constellationArr,^DEBUG("DotsArr")=DotsArr
	set ANS=constellationArr
	//
	//
	//
	quit ANS  //<433
}
CompressConstellation(&DotsArr,&constellationArr)
{
	set constellation=""
	for  {
		set constellation=$order(constellationArr(constellation)) quit:constellation=""
		set Size=constellationArr(constellation)
		set SizeArr(Size,constellation)=""
	}
	set Size="",SW=0
	for  {
		set Size=$order(SizeArr(Size)) quit:Size=""
		set constellation=""
		for  {
			set constellation=$order(SizeArr(Size,constellation)) quit:constellation=""
			set Dot=""
			for  {
				set Dot=$order(constellationArr(constellation,Dot)) quit:Dot=""
				set DotData=DotsArr(Dot)
				set nextSize=""
				for  {
					set nextSize=$order(SizeArr(nextSize),-1) quit:nextSize=""
					//continue:nextSize=Size  //It actually can move (and sometimes should move) between constellations of the same size.
					set nextConstellation=""
					for  {
						set nextConstellation=$order(SizeArr(nextSize,nextConstellation)) quit:nextConstellation=""
						continue:nextConstellation=constellation
						set nextDot=""
						for  {
							set nextDot=$order(constellationArr(nextConstellation,nextDot)) quit:nextDot=""
							set conDist=0,conDotData=DotsArr(nextDot)
							for axis=1:1:4 set conDist=conDist+$zabs($list(conDotData,axis)-$list(DotData,axis))
							if conDist<=3 {
								set SW=1
								set constellationArr(nextConstellation,Dot)=""
								set constellationArr(nextConstellation)=$increment(constellationArr(nextConstellation))
								kill constellationArr(constellation,Dot)
								set constellationArr(constellation)=constellationArr(constellation)-1
								if 'constellationArr(constellation) {
									kill constellationArr(constellation)
									set constellationArr=constellationArr-1
								}
								quit
							}
						}
						quit:SW
					}
					quit:SW
				}
				quit:SW
			}
			quit:SW
		}
		quit:SW
	}
	quit SW
}
MakeConstellation(&DotsArr,&DistArr,&constellationArr)
{
	set Dist=""
	for  {
		set Dist=$order(DistArr(Dist)) quit:Dist=""
		set Dot=""
		for  {
			set Dot=$order(DistArr(Dist,Dot)) quit:Dot=""
			set DotData=DotsArr(Dot)
			set constellation="",SW=0
			for  {
				set constellation=$order(constellationArr(constellation)) quit:constellation=""
				set conDot=""
				for  {
					set conDot=$order(constellationArr(constellation,conDot)) quit:conDot=""
					set conDotData=DotsArr(conDot)
					set conDist=0
					for axis=1:1:4 set conDist=conDist+$zabs($list(conDotData,axis)-$list(DotData,axis))
					if conDist<=3 {
						set SW=1
						set constellationArr(constellation,Dot)=""
						set constellationArr(constellation)=$increment(constellationArr(constellation))
						quit
					}
				}
				quit:SW
			}
			set:'SW constellationArr($increment(constellationArr),Dot)="",constellationArr(constellationArr)=1
		}
	}
}
