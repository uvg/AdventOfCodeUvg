Part1(FileName="",RecordsArr) PUBLIC	;
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		continue:$translate(Record,"v^<>")=Record
		for i=1:1:$length(Record) {
			set ltr=$extract(Record,i)
			continue:$length($translate(ltr,"v^<>"))
			set dir=$select(ltr="v":$listbuild(0,1),ltr="^":$listbuild(0,-1),ltr="<":$listbuild(-1,0),1:$listbuild(1,0))
			set arr(Line,i)=dir_$listbuild(0)
		}
	}
	zwrite arr
	do PrintGrid(.RecordsArr,.arr)
	set ANS=""
	for  {
		kill nextArr
		set Y=""
		for  {
			set Y=$order(arr(Y)) quit:Y=""
			//w Y,!
			set X=""
			for  {
				set X=$order(arr(Y,X)) quit:X=""
				set data=arr(Y,X)
				set dx=$list(data,1),dy=$list(data,2),cnt=$list(data,3)
				set NextX=X+dx
				set NextY=Y+dy
				if $data(arr(NextY,NextX))||$data(nextArr(NextY,NextX)) {
					//write !,X,",",Y,",",NextX,",",NextY,!
					set ANS=$listbuild(NextX-1,NextY-1)
					quit
				}
				set trackLtr=$extract($get(RecordsArr(NextY)),NextX)
				if ("\/"[trackLtr) {
					if dx {
						if dx=1 {
							set dx=0,dy=-1
							set:trackLtr="\" dy=1
						}
						else {
							set dx=0,dy=-1
							set:trackLtr="/" dy=1
						}
					}
					else {
						if dy=1 {
							set dy=0,dx=-1
							set:trackLtr="\" dx=1
						}
						else {
							set dy=0,dx=-1
							set:trackLtr="/" dx=1
						}
					}
				}
				if trackLtr="+" {
					if cnt#3=0 {
						//Turn left
						set dt=dx,dx=dy,dy=-dt
					}
					if cnt#3=2 {
						//Turn right
						set dt=dx,dx=-dy,dy=dt
					}
					set cnt=cnt+1
				}
				kill arr(Y,X)
				set nextArr(NextY,NextX)=$listbuild(dx,dy,cnt)
			}
			quit:$length(ANS)
		}
		quit:$length(ANS)
		merge arr=nextArr
		//write !! zwrite arr
		do PrintGrid(.RecordsArr,.arr)
	}
	//
	//
	//
	quit ANS
}
PrintGrid(&Grid,&Arr)
{
	quit
	set Y=""
	for  {
		set Y=$order(Grid(Y)) quit:Y=""
		write !
		set Record=Grid(Y)
		for X=1:1:$length(Record) {
			set ltr=$extract(Record,X)
			set:"v^"[ltr ltr="|"
			set:"<>"[ltr ltr="-"
			set data=$get(Arr(Y,X))
			if $length(data) {
				set dx=$list(data,1),dy=$list(data,2)
				set:dx ltr=$select(dx=1:">",1:"<")
				set:dy ltr=$select(dy=1:"v",1:"^")
			}
			write ltr
		}
	}
}
