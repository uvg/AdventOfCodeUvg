			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		continue:$translate(Record,"v^<>")=Record
		for i=1:1:$length(Record) {
			set ltr=$extract(Record,i)
			continue:$length($translate(ltr,"v^<>"))
			set dir=$select(ltr="v":$listbuild(0,1),ltr="^":$listbuild(0,-1),ltr="<":$listbuild(-1,0),1:$listbuild(1,0))
			set arr(Line,i)=dir_$listbuild(0)
		}
	}
	zwrite arr
	do PrintGrid(.RecordsArr,.arr)
	set ANS=""
	for  {
		kill nextArr
		set Y=""
		for  {
			set Y=$order(arr(Y)) quit:Y=""
			//w Y,!
			set X=""
			for  {
				set X=$order(arr(Y,X)) quit:X=""
				set data=arr(Y,X)
				set dx=$list(data,1),dy=$list(data,2),cnt=$list(data,3)
				set NextX=X+dx
				set NextY=Y+dy
				if $data(arr(NextY,NextX))||$data(nextArr(NextY,NextX)) {
					kill arr(NextY,NextX),nextArr(NextY,NextX),arr(Y,X)
					continue
				}
				set trackLtr=$extract($get(RecordsArr(NextY)),NextX)
				if ("\/"[trackLtr) {
					if dx {
						if dx=1 {
							set dx=0,dy=-1
							set:trackLtr="\" dy=1
						}
						else {
							set dx=0,dy=-1
							set:trackLtr="/" dy=1
						}
					}
					else {
						if dy=1 {
							set dy=0,dx=-1
							set:trackLtr="\" dx=1
						}
						else {
							set dy=0,dx=-1
							set:trackLtr="/" dx=1
						}
					}
				}
				if trackLtr="+" {
					if cnt#3=0 {
						//Turn left
						set dt=dx,dx=dy,dy=-dt
					}
					if cnt#3=2 {
						//Turn right
						set dt=dx,dx=-dy,dy=dt
					}
					set cnt=cnt+1
				}
				kill arr(Y,X)
				set nextArr(NextY,NextX)=$listbuild(dx,dy,cnt)
			}
		}
		merge arr=nextArr
		//write !! zwrite arr
		do PrintGrid(.RecordsArr,.arr)
		quit:'$data(arr)
		set Y=$order(arr(""))
		if Y=$order(arr(""),-1) {
			set X=$order(arr(Y,""))
			if X=$order(arr(Y,""),-1) {
				set ANS=$listbuild(X-1,Y-1)
				quit
			}
		}
	}
	//
	//
	//
	quit ANS
}
CheckData(RecordsArr)
{
	set Line="",Error=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		if $length($translate(Record," <>v^|-/\+")) {
			set Error="Wrong data format at line "_Line
			quit
		}
	}
	if $length(Error) {
		write !,Error
		quit 0
	}
	quit 1
}
LoadFile(FileName,RecordsArr)	;
{
	set io=$io
	kill RecordsArr
	open FileName::.1 if '$test {
		write !,"Error loading file "_FileName
		quit 0
	}
	set EndOfFileSW=$zutil(68,40,1) //$system.Process.SetZEOF(1)
	use FileName
	for Line=1:1 {
		read Record quit:$zeof
		set RecordsArr(Line)=Record
	}
	close FileName
	if 'EndOfFileSW do $zutil(68,40,0) //$system.Process.SetZEOF(0)
	use io
	quit 1
}
