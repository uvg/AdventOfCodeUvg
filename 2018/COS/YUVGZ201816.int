Part1(FileName="",RecordsArr) PUBLIC	;
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	do LoadOperations(.OperationsArr)
	do AnalayzeData(.RecordsArr,.OperationsArr,.ExamplesArr)
	zwrite OperationsArr,ExamplesArr
	set Total=0,Example=""
	for  {
		set Example=$order(ExamplesArr(Example)) quit:Example=""
		set Data=ExamplesArr(Example)
		set:$listlength($list(Data,4))>=3 Total=Total+1
	}
	set ANS=Total //>487
	//
	//
	//
	quit ANS
}
LoadOperations(&OperationsArr)
{
	set OperationsArr(1)="S r(C)=r(A)+r(B)"
	set OperationsArr(2)="S r(C)=r(A)+B"
	set OperationsArr(3)="S r(C)=r(A)*r(B)"
	set OperationsArr(4)="S r(C)=r(A)*B"
	set OperationsArr(5)="S r(C)=$$AND(r(A),r(B))"
	set OperationsArr(6)="S r(C)=$$AND(r(A),B)"
	set OperationsArr(7)="S r(C)=$$OR(r(A),r(B))"
	set OperationsArr(8)="S r(C)=$$OR(r(A),B)"
	set OperationsArr(9)="S r(C)=r(A)"
	set OperationsArr(10)="S r(C)=A"
	set OperationsArr(11)="S r(C)=A>r(B)"
	set OperationsArr(12)="S r(C)=r(A)>B"
	set OperationsArr(13)="S r(C)=r(A)>r(B)"
	set OperationsArr(14)="S r(C)=A=r(B)"
	set OperationsArr(15)="S r(C)=r(A)=B"
	set OperationsArr(16)="S r(C)=r(A)=r(B)"
}
AND(A,B) PUBLIC
{
	set val1=$factor(A),val2=$factor(B)
	quit $$FactorToDec($bitlogic(val1&val2))
}
OR(A,B) PUBLIC
{
	set val1=$factor(A),val2=$factor(B)
	quit $$FactorToDec($bitlogic(val1|val2))
}
FactorToDec(Factor)
{
	set ans=0,mul=1
	for i=1:1:$bitcount(Factor) {
		set:$bit(Factor,i) ans=ans+mul
		set mul=mul*2
	}
	quit ans	
}
AnalayzeData(&RecordsArr,&OperationsArr,&ExamplesArr,&InstructionsArr)
	new (RecordsArr,OperationsArr,ExamplesArr,InstructionsArr)
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		continue:'$length(Record)
		if Record["Before: [" {
			set Record1=Record,Record2=RecordsArr(Line+1),Record3=RecordsArr(Line+2)
			set data=$piece($piece(Record1,"[",2),"]"),part1="" for i=1:1:4 set part1=part1_$listbuild(+$piece(data,", ",i))
			set data=Record2,part2="" for i=1:1:4 set part2=part2_$listbuild(+$piece(data," ",i))
			set data=$piece($piece(Record3,"[",2),"]"),part3="" for i=1:1:4 set part3=part3_$listbuild(+$piece(data,", ",i))
			set Op="",Maybe=""
			for i=0:1:3 set OrgR(i)=$list(part1,i+1)
			set A=$list(part2,2),B=$list(part2,3),C=$list(part2,4)
			//write !,A,";",B,";",C,!
			for  {
				set Op=$order(OperationsArr(Op)) quit:Op=""
				set Operation=OperationsArr(Op)
				merge r=OrgR
				//write !,Line,">>>",A,";",B,";",C," Before:",!
				//zwrite part1,part2,part3,Operation,r
				xecute Operation
				//write !," After:",!
				//zwrite Operation,r				
				set sw=1 for i=0:1:3 if r(i)'=$list(part3,i+1) set sw=0 quit
				set:sw Maybe=Maybe_$listbuild(Op)
			}
			set str=$listbuild(part2,part1,part3,Maybe)
			set ExamplesArr($increment(ExamplesArr))=str
			set Line=Line+3
			continue
		}
		set data=Record,str="" for i=1:1:4 set str=str_$listbuild(+$piece(data," ",i))
		set InstructionsArr($increment(InstructionsArr))=str
	}
	quit
