Part2(FileName="",RecordsArr) PUBLIC	;
	new (FileName,RecordsArr)
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	do LoadOperations(.OperationsArr)
	do AnalayzeData(.RecordsArr,.OperationsArr,.ExamplesArr,.InstructionsArr)
	//zwrite OperationsArr,ExamplesArr,InstructionsArr
	do GetOperations(.ExamplesArr,.OperationsArr,.OpCodesArr)
	zwrite OpCodesArr,InstructionsArr
	for i=0:1:3 set r(i)=0
	set Instruction=""
	for  {
		set Instruction=$order(InstructionsArr(Instruction)) quit:Instruction=""
		set Data=InstructionsArr(Instruction)
		set Op=OpCodesArr($list(Data,1)),A=$list(Data,2),B=$list(Data,3),C=$list(Data,4)
		xecute Op
	}
	set ANS=r(0)
	//
	//
	//
	quit ANS

GetOperations(&ExamplesArr,&OperationsArr,&OpCodesArr)
{
	set Example=""
	for  {
		set Example=$order(ExamplesArr(Example)) quit:Example=""
		set Data=ExamplesArr(Example)
		set Op=$list($list(Data,1),1),Maybe=$list(Data,4)
		set:'$data(Maybe(Op)) Maybe(Op)=Maybe
		set str=""
		for i=1:1:$listlength(Maybe(Op)) if $listfind(Maybe,$list(Maybe(Op),i)) set str=str_$listbuild($list(Maybe(Op),i))
		set:str'=Maybe(Op) Maybe(Op)=str
	}
	zwrite Maybe write !!!
	for  {
		set Op="",SW=0
		for  {
			set Op=$order(Maybe(Op)) quit:Op=""
			set Data=Maybe(Op)
			continue:$listlength(Data)>1
			set ToOp=$list(Data,1)
			set tempOp=""
			for  {
				set tempOp=$order(Maybe(tempOp)) quit:tempOp=""
				continue:tempOp=Op
				set Data=Maybe(tempOp)
				set find=$listfind(Data,ToOp)
				continue:'find
				set str=$list(Data,1,find-1)_$list(Data,find+1,*)
				set Maybe(tempOp)=str,SW=1
			}
		}
		quit:'SW
	}
	zwrite Maybe,OperationsArr
	set Maybe="" for  {
		set Maybe=$order(Maybe(Maybe)) quit:Maybe=""
		set OpCodesArr(Maybe)=OperationsArr($list(Maybe(Maybe),1))		
	}
}
CheckData(RecordsArr)
{
	set Line="",Error=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		continue:'$length(Record)
		if Record'?1(1(1"Before: [",1"After:  [")1.N1", "1.N1", "1.N1", "1.N1"]",1.N1" "1.N1" "1.N1" "1.N) {
			set Error="Wrong data format at line "_Line
			quit
		}
	}
	if $length(Error) {
		write !,Error
		quit 0
	}
	quit 1
}
LoadFile(FileName,RecordsArr)	;
{
	kill RecordsArr
	open FileName::.1 if '$test {
		write !,"Error loading file "_FileName
		quit 0
	}
	set EndOfFileSW=$zutil(68,40,1) //$system.Process.SetZEOF(1)
	use FileName
	for Line=1:1 {
		read Record quit:$zeof
		set RecordsArr(Line)=Record
	}
	close FileName
	if 'EndOfFileSW do $zutil(68,40,0) //$system.Process.SetZEOF(0)
	quit 1
}
