W $$Part1^YUVGZ201820(FN)
	*/
Part1(FileName="",RecordsArr) PUBLIC	;
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set MinFrameStack=1024*1024
	if $System.Util.GetFrameStackSize()<MinFrameStack {
		do $System.Util.SetFrameStackSize(MinFrameStack)
		write !,"Open a new process with the extended frame stack"
		quit 0
	}
	set Route=$extract(RecordsArr(1),2,*-1),StartX=0,StartY=0
	//write !,Route,!
	do SetGrid(.Grid,Route,StartX,StartY)
	set Grid(StartY,StartX)="X"
	//do PrintGrid(.Grid)
	//zwrite Grid
	set ANS=$$FindPathWithMaxDoors(.Grid,StartX,StartY,0)-1
	//
	//
	//
	quit ANS
}
FindPathWithMaxDoors(&Grid,X,Y,DoorsCounter,&Visited)
{
	quit:$data(Visited(Y,X)) DoorsCounter
	set Visited(Y,X)=DoorsCounter,MaxDoors=DoorsCounter
	set ltr=$get(Grid(Y-1,X)) if ltr="-" set doors=$$FindPathWithMaxDoors(.Grid,X,Y-2,DoorsCounter+1,.Visited) set:doors>MaxDoors MaxDoors=doors
	set ltr=$get(Grid(Y+1,X)) if ltr="-" set doors=$$FindPathWithMaxDoors(.Grid,X,Y+2,DoorsCounter+1,.Visited) set:doors>MaxDoors MaxDoors=doors
	set ltr=$get(Grid(Y,X-1)) if ltr="|" set doors=$$FindPathWithMaxDoors(.Grid,X-2,Y,DoorsCounter+1,.Visited) set:doors>MaxDoors MaxDoors=doors
	set ltr=$get(Grid(Y,X+1)) if ltr="|" set doors=$$FindPathWithMaxDoors(.Grid,X+2,Y,DoorsCounter+1,.Visited) set:doors>MaxDoors MaxDoors=doors
	quit MaxDoors
}
PrintGrid(&Grid)
{
	set minX=999999999999,maxX=-minX,Y=""
	for  {
		set Y=$order(Grid(Y)) quit:Y=""
		set X=$order(Grid(Y,""))
		set:X<minX minX=X
		set X=$order(Grid(Y,""),-1)
		set:X>maxX maxX=X
	}
	for Y=$order(Grid(""))-1:1:$order(Grid(""),-1)+1 {
		write !
		for X=minX-1:1:maxX+1 {
			set ltr=$get(Grid(Y,X))
			set:'$length(ltr) ltr="#"
			write ltr
		}
	}
	write !!!
}
SetGrid(&Grid,Route,X,Y)
{
	set tempX=X,tempY=Y
	for i=1:1:$length(Route) {
		set ltr=$extract(Route,i)
		if ltr'="(",ltr'="|" {
			set dx=$select(ltr="E":1,ltr="W":-1,1:0)
			set dy=$select(ltr="S":1,ltr="N":-1,1:0)
			set tempX=tempX+dx
			set tempY=tempY+dy
			set Grid(tempY,tempX)=$select(dx:"|",1:"-")
			set tempX=tempX+dx
			set tempY=tempY+dy
			set Grid(tempY,tempX)="."
		}
		elseif ltr="(" {
			set cnt=0
			for j=i:1 {
				set:$extract(Route,j)="(" cnt=cnt+1
				set:$extract(Route,j)=")" cnt=cnt-1
				quit:cnt=0
			}
			set innerRoute=$extract(Route,i+1,j-1)
			set i=j
			//w !,innerRoute
			do SetGrid(.Grid,innerRoute,tempX,tempY)
		}
		else {
			set tempX=X,tempY=Y
		}
	}
}
