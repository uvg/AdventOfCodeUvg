	/*
W $$Part2^YUVGZ201820(FN)
	*/
Part2(FileName="",RecordsArr) PUBLIC	;
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set MinFrameStack=1024*1024
	if $System.Util.GetFrameStackSize()<MinFrameStack {
		do $System.Util.SetFrameStackSize(MinFrameStack)
		write !,"Open a new process with the extended frame stack"
		quit 0
	}
	set Route=$extract(RecordsArr(1),2,*-1),StartX=0,StartY=0
	//write !,Route,!
	do SetGrid(.Grid,Route,StartX,StartY)
	set Grid(StartY,StartX)="X"
	//do PrintGrid(.Grid)
	//zwrite Grid
	do FindPathWithMaxDoors(.Grid,StartX,StartY,0,.Visited)
	zwrite Visited
	set Y="",Total=0,MinDoors=1000
	for  {
		set Y=$order(Visited(Y)) quit:Y=""
		set X=""
		for  {
			set X=$order(Visited(Y,X)) quit:X=""
			set:Visited(Y,X)>=MinDoors Total=Total+1
		}
	}
	set ANS=Total
	//
	//
	//
	quit ANS
}
CheckData(RecordsArr)
{
	if ($order(RecordsArr(""))'=1)||($order(RecordsArr(""),-1)'=1)
	{
		write !,"Only one line of data allowed"
		quit 0
	}
	set Line="",Error=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		if Record'?1"^"1.E1"$" {
			set Error="Wrong data format at line "_Line
			quit
		}
		if $length($translate($extract(Record,2,*-1),"NEWS()|")) {
			set Error="Wrong data format at line "_Line
			quit
		}
	}
	if $length(Error) {
		write !,Error
		quit 0
	}
	quit 1
}
LoadFile(FileName,RecordsArr)	;
{
	set io=$io
	kill RecordsArr
	open FileName::.1 if '$test {
		write !,"Error loading file "_FileName
		quit 0
	}
	set EndOfFileSW=$zutil(68,40,1) //$system.Process.SetZEOF(1)
	use FileName
	for Line=1:1 {
		read Record quit:$zeof
		set RecordsArr(Line)=Record
	}
	close FileName
	if 'EndOfFileSW do $zutil(68,40,0) //$system.Process.SetZEOF(0)
	use io
	quit 1
}
