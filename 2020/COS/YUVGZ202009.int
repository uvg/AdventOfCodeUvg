			}
		}
		quit:SW
	}
	for Line=$piece(SW,"*",1):1:$piece(SW,"*",2) {
		set ContigousSet(RecordsArr(Line))=""
	}
}
GetTotalForArray(&RecordsArr,From,To)
{
	set Line=From-1,Total=0
	for  {
