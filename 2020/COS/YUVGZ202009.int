}
FindContigousSet(&RecordsArr,Number,&ContigousSet)
{
	set SW=0
	set Line="" for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Line2=Line
		for  {
			set Line2=$order(RecordsArr(Line2)) quit:Line2=""
			set Total=$$GetTotalForArray(.RecordsArr,Line,Line2)
			quit:Total>Number
			if Total=Number {
