Part1(FileName="",RecordsArr) PUBLIC
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Moves=RecordsArr(Line)
		set (X,Y,Z)=0
		do GetLocation(Moves,.X,.Y,.Z)
		set Tiles(X,Y,Z)='$get(Tiles(X,Y,Z))
		//w !!!,Moves,":"
	}
	//w !!!
	//zwrite Tiles
	set Ans=$$HowManyBlackTiles(.Tiles)
	//
	//
	//
	//
	quit Ans
}
GetLocation(Moves,X,Y,Z)
{
	for i=1:1:$length(Moves) {
		set Move=$extract(Moves,i)
		set:"sn"[Move Move=Move_$extract(Moves,i+1),i=i+1
		//w !,Move
		do Move(.X,.Y,.Z,Move)
	}
}
Move(&X,&Y,&Z,Move)
{
	set X=$case(Move,"w":-1,"e":1,"ne":1,"sw":-1,:0)+X
	set Y=$case(Move,"nw":1,"w":1,"e":-1,"se":-1,:0)+Y
	set Z=$case(Move,"nw":-1,"ne":-1,"sw":1,"se":1,:0)+Z
}	
HowManyBlackTiles(&Tiles)
{
	set Total=0
	set X=""
	for  {
		set X=$order(Tiles(X)) quit:X=""
		set Y=""
		for  {
			set Y=$order(Tiles(X,Y)) quit:Y=""
			set Z=""
			for  {
				set Z=$order(Tiles(X,Y,Z)) quit:Z=""
				set:Tiles(X,Y,Z) Total=Total+1
			}
		}
	}
	quit Total
}
	/*
Day 24 part 2:
The tile floor in the lobby is meant to be a living art exhibit. Every day, the tiles are all flipped according to the following rules:

Any black tile with zero or more than 2 black tiles immediately adjacent to it is flipped to white.
Any white tile with exactly 2 black tiles immediately adjacent to it is flipped to black.
Here, tiles immediately adjacent means the six tiles directly touching the tile in question.

The rules are applied simultaneously to every tile; put another way, it is first determined which tiles need to be flipped, then they are all flipped at the same time.

In the above example, the number of black tiles that are facing up after the given number of days has passed is as follows:

Day 1: 15
Day 2: 12
Day 3: 25
Day 4: 14
Day 5: 23
Day 6: 28
Day 7: 41
Day 8: 37
Day 9: 49
Day 10: 37

Day 20: 132
Day 30: 259
Day 40: 406
Day 50: 566
Day 60: 788
Day 70: 1106
Day 80: 1373
Day 90: 1844
Day 100: 2208
After executing this process a total of 100 times, there would be 2208 black tiles facing up.

How many tiles will be black after 100 days?
	*/
	/*
W $$Part2^YUVGZ202024(FN)
	*/
Part2(FileName="",RecordsArr) PUBLIC
{
	if $data(RecordsArr)'=10 {
		if '$length(FileName) {
			read !,"Enter filename: ",FileName
			write !
		}
		if '$length(FileName) {
			write !,"Missing file to read from"
			quit 0
		}
		quit:'$$LoadFile(FileName,.RecordsArr) 0
		if '$data(RecordsArr) {
			write !,"No data was red from "_FileName
			quit 0
		}
	}
	quit:'$$CheckData(.RecordsArr) 0
	set Line=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Moves=RecordsArr(Line)
		//w !!!,Moves,":"
		set (X,Y,Z)=0
		do GetLocation(Moves,.X,.Y,.Z)
		set Tiles(X,Y,Z)='$get(Tiles(X,Y,Z))
	}
	//write !,"Before:",!
	//zwrite Tiles
	//write !!,"After:",!
	for i=1:1:100 do FilpTiles(.Tiles) //w i,! // /*zwrite Tiles*/ write !,i,"=",$$HowManyBlackTiles(.Tiles),!
	//w !!!
	//zwrite Tiles
	set Ans=$$HowManyBlackTiles(.Tiles)
	//
	//
	//
	//
	quit Ans
}
FilpTiles(&Tiles)
{
	set MaxBorder=$$FindBorder(.Tiles)
	//write !,MaxBorder
	merge tempTiles=Tiles
	for X=-MaxBorder-1:1:MaxBorder+1 {
		for Y=-MaxBorder-1:1:MaxBorder+1 {
			for Z=-MaxBorder-1:1:MaxBorder+1 {
				set Tile=$get(Tiles(X,Y,Z))
				set adj=$$GetAdjust(.Tiles,X,Y,Z)
				if Tile,adj>2||'adj set tempTiles(X,Y,Z)=0
				if 'Tile,adj=2 set tempTiles(X,Y,Z)=1
			}
		}
	}
	kill Tiles
	merge Tiles=tempTiles
}
GetAdjust(&Tiles,X,Y,Z)
{
	set Total=0
	for Move="e","w","ne","nw","se","sw" {
		set tempX=X,tempY=Y,tempZ=Z
		do Move(.tempX,.tempY,.tempZ,Move)
		set:$get(Tiles(tempX,tempY,tempZ)) Total=Total+1
	}
	quit Total
}
FindBorder(&Tiles)
{
	set Border=0
	set X=""
	for  {
		set X=$order(Tiles(X)) quit:X=""
		set Y=""
		for  {
			set Y=$order(Tiles(X,Y)) quit:Y=""
			set Z=""
			for  {
				set Z=$order(Tiles(X,Y,Z)) quit:Z=""
				if $zabs(Z)>Border,Tiles(X,Y,Z) set Border=$zabs(Z)
			}
			if $zabs(Y)>Border set Border=$zabs(Y)
		}
		set:$zabs(X)>Border Border=$zabs(X)
	}
	quit Border
}
CheckData(RecordsArr)
{
	set Line="",Error=""
	for  {
		set Line=$order(RecordsArr(Line)) quit:Line=""
		set Record=RecordsArr(Line)
		if $length($translate(Record,"snwe")) {
			set Error="Wrong data format at line "_Line
			quit
		}
	}
	if $length(Error) {
		write !,Error
		quit 0
	}
	quit 1
}
LoadFile(FileName,RecordsArr)
{
	kill RecordsArr
	open FileName::.1 if '$test {
		write !,"Error loading file "_FileName
		quit 0
	}
	set EndOfFileSW=$zutil(68,40,1) //$system.Process.SetZEOF(1)
	use FileName
	for Line=1:1 {
		read Record quit:$zeof
		if $length(Record) set RecordsArr(Line)=Record
	}
	close FileName
	if 'EndOfFileSW do $zutil(68,40,0) //$system.Process.SetZEOF(0)
	quit 1
}
